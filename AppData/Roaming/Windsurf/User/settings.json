	"vue.complete.defineAssignment": true,
	"vue.complete.casing.props": "autoKebab",
	"vue.codeLens.enabled": true,
	"vue.complete.casing.tags": "autoPascal",
	"vue.splitEditors.icon": false,
	"vue.inlayHints.destructuredProps": true,
	"vue.inlayHints.optionsWrapper": true,
	//"vue.inlayHints.inlineHandlerLeading": true,
	"vue.trace.server": "off",
	"vue.server.includeLanguages": ["vue"],
	"vue.splitEditors.layout.left": ["script", "scriptSetup", "styles"],
	"vue.splitEditors.layout.right": ["template"],

	"code-runner.saveAllFilesBeforeRun": true,
	"code-runner.saveFileBeforeRun": true,
	"CodeGPT.apiKey": "Groq",
	"codeium.enableCodeLens": false,
	"codeium.enableConfig": {
		"*": true,
		"markdown": true
	},
	"codesnap-plus.containerPadding": "1em",
