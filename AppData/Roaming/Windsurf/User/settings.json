	"aicommit.endpoint": "https://api.deepseek.com",
	"aicommit.model": "deepseek-coder",
	"aicommit.provider": "openai-compatible",
	"aide.aiCommand": "aider #{filesRelativePath}",
	"aide.openaiKey": "",
	"aide.openaiModel": "gpt-4o",
